ul.disc 
  list-style-type: disc 

ul.circle 
  list-style-type: circle 

ul.square 
  list-style-type: square 

ol.lower-roman 
  list-style-type: lower-roman 

ol.upper-roman 
  list-style-type: upper-roman 

ol.lower-alpha 
  list-style-type: lower-alpha 

ol.upper-alpha 
  list-style-type: upper-alpha 

ol.cn 
  list-style-type: cjk-ideographic 

.li-inline 
  padding-left: 0 

.li-inline > li,
.li-inline > .li-simple,
.li-inline > .li-complex 
  display: inline-block 
  .li-inline > li:not(:last-child),
  .li-inline > .li-simple:not(:last-child),
  .li-inline > .li-complex:not(:last-child) 
    margin-right: 5px 

@media (min-width: 768px) 
  .dl-h dt 
    width: 15%
    display: inline-block
    float: left
    text-align: right
    white-space: nowrap
    text-overflow: ellipsis
    overflow: hidden 

  .dl-h dd 
    width: 85%
    display: inline-block
    float: left
    padding: 0 0 15px 15px

ul.list,
ol.list,
.list 
  list-style-type: none
  padding: 0
  margin: 1em 0em 

ul.list li,
ol.list li,
.li-simple,
.li-complex 
  display: list-item
  table-layout: fixed
  list-style-type: none
  list-style-position: outside
  line-height: 1.5em 

.list .list > .li-simple 
  padding-left: 1.5em 

/* Child List */
ul.list ul,
ol.list ol,
.list .list 
  clear: both
  margin: 0
  padding: 0 

ul.list:first-child,
ol.list:first-child,
.list:first-child 
  margin-top: 0 

ul.list:last-child,
ol.list:last-child,
.list:last-child 
  margin-bottom: 0 

.li-complex 
  width: 100%
  padding: 0.75rem 1.25rem
  margin-bottom: -1px
  color: #555
  background-color: #fff
  text-align: left 
  .li-complex:first-child 
    border-top-right-radius: 0.25rem
    border-top-left-radius: 0.25rem 
  .li-complex:last-child 
    margin-bottom: 0
    border-bottom-right-radius: 0.25rem
    border-bottom-left-radius: 0.25rem 
  .li-complex .li-header 
    color: #333 
  .li-complex:focus, .li-complex:focus *, .li-complex:hover 
    color: #e2e2e2
    text-decoration: none
    background-color: #818a91 
  .li-complex.disabled, .li-complex.disabled:focus, .li-complex.disabled:hover 
    color: #818a91
    cursor: not-allowed
    background-color: #e2e2e2 
    .li-complex.disabled .li-header, .li-complex.disabled:focus .li-header, .li-complex.disabled:hover .li-header 
      color: inherit 
    .li-complex.disabled .li-text, .li-complex.disabled:focus .li-text, .li-complex.disabled:hover .li-text 
      color: #818a91 
  .li-complex.active, .li-complex.active:focus, .li-complex.active:hover 
    z-index: 2
    color: #fff
    text-decoration: none
    background-color: #35B8E0
    border-color: #35B8E0 
    .li-complex.active .li-header, .li-complex.active:focus .li-header, .li-complex.active:hover .li-header 
      color: inherit 
    .li-complex.active .li-text, .li-complex.active:focus .li-text, .li-complex.active:hover .li-text 
      color: #e6f6fb 

button.li-complex 
  border: 1px solid #ddd
  outline: none 

/* Child Item */
.list .list > .li-complex 
  border: none 

.list .content > .list > .li-simple,
.list .content > .list > .li-complex 
  padding: 0 

.list.br0 .li-complex
  border-radius: 0 

.li-simple > i.ic,
.li-complex > i.ic,
.li-simple > .image,
.li-complex > .image 
  display: table-cell
  padding-top: .1em
  margin: 0 

.li-simple > i.ic:only-child,
.li-simple > .image:only-child,
.li-simple > img.image,
.li-complex > i.ic:only-child,
.li-complex > .image:only-child,
.li-complex > img.image 
  display: inline-block 

.li-simple > .image,
.li-complex > .image,
.li-simple > .image img,
.li-complex > .image img 
  vertical-align: top 

.li-simple > .ic + .content,
.li-simple > .image + .content,
.li-complex > .ic + .content,
.li-complex > .image + .content 
  display: table-cell
  padding: 0 0 0 .2em 

.li-simple > img.image + .content,
.li-complex > img.image + .content 
  display: inline-block 

.li-header 
  margin-top: 0
  margin-bottom: 5px 

.li-text 
  margin-bottom: 0
  line-height: 1.3 

ol.list,
.ordered.list,
.ordered.list .list,
ol.list ol 
  counter-reset: ordered
  margin-left: 1.25rem
  list-style-type: none 

ol.list li,
.ordered.list .list > .li-simple,
.ordered.list > .li-simple 
  list-style-type: none
  position: relative
  padding-left: .3em 

.ordered.list .list > .li-complex,
.ordered.list > .li-complex 
  list-style-type: none
  position: relative
  padding-left: 1.55em 

ol.list li:before,
.ordered.list .list > .li-simple:before,
.ordered.list .list > .li-complex:before,
.ordered.list > .li-simple:before,
.ordered.list > .li-complex:before 
  position: absolute
  top: auto
  left: auto
  user-select: none
  pointer-events: none
  margin-left: -1.55rem
  counter-increment: ordered
  content: counters(ordered,".") " "
  text-align: right
  color: rgba(0, 0, 0, 0.87)
  vertical-align: middle
  opacity: 0.8 

/* Value */
.ordered.list .list > .li-simple[data-value]:before,
.ordered.list > .li-simple[data-value]:before,
.ordered.list .list > .li-complex[data-value]:before,
.ordered.list > .li-complex[data-value]:before 
  content: attr(data-value) 

ol.list li[value]:before 
  content: attr(value) 